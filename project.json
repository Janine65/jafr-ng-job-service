{
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "name": "job-service",
  "projectType": "library",
  "sourceRoot": "src_job/src",
  "prefix": "lib",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "options": {
        "project": "src_job/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "src_job/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "src_job/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "src_job/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "src_job/**/*.ts",
          "src_job/**/*.html"
        ]
      }
    }
  },
  "tags": []
}