{
  "name": "kpm-ng",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run generate-test-manifest",
    "start": "nx serve",
    "build": "nx build",
    "build:prod": "nx build:prod",
    "watch": "nx build --watch --configuration development",
    "lint": "nx lint",
    "format": "prettier --write \"**/*.{js,mjs,ts,mts,d.ts,html}\" --cache",
    "postinstall": "node scripts/setup-core.cjs",
    "check-sheetjs-update": "ts-node ./scripts/check-sheetjs-update.ts",
    "check-translations": "ts-node scripts/check-translations.ts",
    "generate-models": "ts-node scripts/generate-models.ts",
    "generate-services": "ts-node scripts/generate-services.ts",
    "generate-test-manifest": "node scripts/generate-test-manifest.js",
    "serve:dev": "npm run copy-env:dev && nx serve",
    "serve:mock": "npm run copy-env:mock && nx serve",
    "serve:syst": "npm run copy-env:syst && nx serve",
    "serve:intg": "npm run copy-env:intg && nx serve",
    "serve:prod": "npm run copy-env:prod && nx serve",
    "copy-env:dev": "cp src/environments/environment.dev.json src/assets/config/environment.json",
    "copy-env:mock": "cp src/environments/environment.mock.json src/assets/config/environment.json",
    "copy-env:syst": "cp src/environments/environment.syst.json src/assets/config/environment.json",
    "copy-env:intg": "cp src/environments/environment.intg.json src/assets/config/environment.json",
    "copy-env:prod": "cp src/environments/environment.prod.json src/assets/config/environment.json",
    "env:lock": "git update-index --skip-worktree src/assets/config/environment.json",
    "env:unlock": "git update-index --no-skip-worktree src/assets/config/environment.json",
    "env:status": "git ls-files -v src/assets/config/environment.json",
    "env:reset-to-mock": "npm run copy-env:mock && npm run env:lock",
    "libs:watch": "node scripts/libs-watch.cjs",
    "libs:build": "node scripts/libs-build.cjs",
    "libs:rebuild": "node scripts/libs-build.cjs --clean",
    "cache:clear": "rm -rf node_modules .angular dist && rm -rf ~/.pnpm-store/v3/tmp && pnpm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.3.9",
    "@angular/cdk": "20.2.11",
    "@angular/common": "20.3.9",
    "@angular/compiler": "20.3.9",
    "@angular/core": "20.3.9",
    "@angular/forms": "20.3.9",
    "@angular/platform-browser": "20.3.9",
    "@angular/platform-browser-dynamic": "20.3.9",
    "@angular/router": "20.3.9",
    "@ngrx/effects": "^20.1.0",
    "@ngrx/signals": "^20.1.0",
    "@ngrx/store": "^20.1.0",
    "@ngrx/store-devtools": "^20.1.0",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^16.0.1",
    "@primeng/themes": "^20.3.0",
    "@primeuix/styled": "^0.7.4",
    "@primeuix/styles": "^1.2.5",
    "@primeuix/themes": "^1.2.5",
    "@primeuix/utils": "^0.6.2",
    "@syrius/core": "^1.0.15",
    "@syrius/data-table": "^1.0.1",
    "@syrius/job-service": "^1.0.2",
    "@tailwindcss/postcss": "^4.1.10",
    "angular-in-memory-web-api": "^0.19.0",
    "chart.js": "4.4.2",
    "html2canvas-pro": "^1.5.11",
    "keycloak-angular": "^20.0.0",
    "keycloak-js": "^26.2.0",
    "ngrx-store-localstorage": "^19.0.0",
    "primeclt": "^0.1.5",
    "primeicons": "^7.0.0",
    "primeng": "20.3.0",
    "rxjs": "~7.8.0",
    "tailwindcss-primeui": "^0.5.1",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zod": "^4.1.12",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.8",
    "@angular-devkit/core": "^19.0.6",
    "@angular-devkit/schematics": "^19.0.6",
    "@angular-eslint/builder": "^20.1.1",
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/eslint-plugin-template": "^20.1.1",
    "@angular-eslint/template-parser": "^20.1.1",
    "@angular/cli": "^20.3.8",
    "@angular/compiler-cli": "20.3.9",
    "@eslint/eslintrc": "^3.3.1",
    "@nx/angular": "21.2.0",
    "@nx/linter": "^19.8.4",
    "@nx/workspace": "21.2.0",
    "@schematics/angular": "^20.3.8",
    "@types/glob": "^8.1.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "angular-eslint": "^20.1.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nx": "21.2.0",
    "postcss": "^8.4.49",
    "prettier": "^3.5.3",
    "semver": "^7.7.2",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.34.1"
  }
}
